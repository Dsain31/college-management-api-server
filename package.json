{
  "name": "college-management-api-server",
  "version": "1.0.0",
  "description": "api-server",
  "main": "public/index.js",
  "scripts": {
    "build": "npm run tsc",
    "tsc": "tsc --project tsconfig.json",
    "start:dev": "nodemon --watch 'src/**/*.ts' --ignore 'node_modules/**/*' --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start":"npm run build && cross-env NODE_ENV=production node public/index.js",
    "start:prod": "npm run build && cross-env NODE_ENV=production node public/index.js"
  },
  "engines": {
    "node": "12.x"
  },
  "author": "deepak sain",
  "license": "ISC",
  "_moduleAliases": {
    "@lib": "public/lib",
    "@config": "public/config",
    "@controllers": "public/api/controllers",
    "@models": "public/api//models",
    "@services": "public/api/services",
    "@repository": "public/api/repository",
    "@utils": "public/api/utils",
    "@interfaces": "public/interfaces"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-useragent": "^1.0.1",
    "@types/lodash": "^4.14.172",
    "@typescript-eslint/eslint-plugin": "^4.31.0",
    "@typescript-eslint/parser": "^4.31.0",
    "eslint": "^7.32.0",
    "ts-node": "^10.2.1",
    "tsconfig-paths": "^3.11.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "helmet": "^4.6.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongodb": "^4.1.1"
  }
}
